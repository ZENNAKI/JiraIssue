{template .pageMockForQUnit}
    <form action="SetupLicense.jspa" class="aui" id="jira-setupwizard" method="post">
        <div class="form-body">
            <h2>
                Specify Your License Key
            </h2>
            <div>
                <p>
                    You need a license key to set up JIRA. Enter your license key or generate a new license key below. You need an account at <a href="http://my.atlassian.com" target="_blank">my.atlassian.com</a> to generate a license key.
                </p>
            </div>
            <div id="license-radio-options" class="inline-radio-options">
                <fieldset>
                    <div class="radio">
                        <input class="radio" id="jira-setupwizard-licenseSetupSelectornewAccount" name="licenseSetupSelector" type="radio" value="newAccount">
                        <label for="jira-setupwizard-licenseSetupSelectornewAccount">
                            I don't have an account
                        </label>
                    </div>
                    <div class="radio">
                        <input class="radio" id="jira-setupwizard-licenseSetupSelectorlogin" name="licenseSetupSelector" type="radio" value="login">
                        <label for="jira-setupwizard-licenseSetupSelectorlogin">
                            I have an account but no key
                        </label>
                    </div>
                    <div class="radio">
                        <input class="radio" id="jira-setupwizard-licenseSetupSelectorexistingLicense" name="licenseSetupSelector" type="radio" value="existingLicense">
                        <label for="jira-setupwizard-licenseSetupSelectorexistingLicense">
                            I have a JIRA key
                        </label>
                    </div>
                </fieldset>
            </div>

            <div id="no-connection-warning" class="aui-message warning shadowed hidden">
                <p class="title">
                    <span class="aui-icon icon-warning"></span>
                    <strong>Unable to reach my.atlassian.com</strong>
                </p>
                <p>
                    We can't contact <a href="https://id.atlassian.com" target="_blank">my.atlassian.com</a>. Make sure you're connected to the Internet and try again. Otherwise, go to <a href="https://id.atlassian.com" target="_blank">my.atlassian.com</a>, create an account and generate a license. When you have the license key, enter it in the box below.
                </p>
            </div>

            <div id="license-input-container" class="license-input-container hidden"></div>
        </div>
    </form>
{/template}